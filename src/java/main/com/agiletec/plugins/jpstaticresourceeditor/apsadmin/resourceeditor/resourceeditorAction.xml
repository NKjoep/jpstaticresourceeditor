<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
    "http://struts.apache.org/dtds/struts-2.1.dtd">

<struts>

	<package name="jpstaticresourceeditor_do/jpstaticresourceeditor/resourceeditor" namespace="/do/jpstaticresourceeditor/Resourceeditor" extends="japs-default">

		<action name="intro" class="jpstaticresourceeditorResourceeditorAction">
			<result type="tiles">admin.jpstaticresourceeditor.resourceeditor.intro</result>
			<interceptor-ref name="japsDefaultStack"><param name="requestAuth.requiredPermission">superuser</param></interceptor-ref>
		</action>	

		<action name="list" class="jpstaticresourceeditorResourceeditorAction" method="list">
			<result type="tiles">admin.jpstaticresourceeditor.resourceeditor.list</result>
			<interceptor-ref name="japsDefaultStack"><param name="requestAuth.requiredPermission">superuser</param></interceptor-ref>
		</action>

		<action name="edit" class="jpstaticresourceeditorResourceeditorAction" method="edit">
			<result type="tiles">admin.jpstaticresourceeditor.resourceeditor.edit</result>
			<result name="failure" type="tiles">admin.jpstaticresourceeditor.resourceeditor.edit</result>
			<interceptor-ref name="japsDefaultStack"><param name="requestAuth.requiredPermission">superuser</param></interceptor-ref>
		</action>

		<action name="save" class="jpstaticresourceeditorResourceeditorAction" method="save">
			 <result name="input" type="tiles">admin.jpstaticresourceeditor.resourceeditor.edit</result>
			 <result type="redirect-action">
				<param name="actionName">list</param>
			 </result>
			<interceptor-ref name="japsDefaultStack"><param name="requestAuth.requiredPermission">superuser</param></interceptor-ref>
		</action>

		<action name="trash" class="jpstaticresourceeditorResourceeditorAction">
			 <result type="tiles">admin.jpstaticresourceeditor.resourceeditor.trash</result>
			<interceptor-ref name="japsDefaultStack"><param name="requestAuth.requiredPermission">superuser</param></interceptor-ref>
		</action>

		<action name="delete" class="jpstaticresourceeditorResourceeditorAction" method="delete">
			 <result name="input" type="tiles">admin.jpstaticresourceeditor.resourceeditor.list</result>
			 <result type="redirect-action">
			 	<param name="actionName">list</param>
			 </result>
			<interceptor-ref name="japsDefaultStack"><param name="requestAuth.requiredPermission">superuser</param></interceptor-ref>
		</action>

		<action name="new" class="jpstaticresourceeditorResourceeditorAction">
			<result type="tiles">admin.jpstaticresourceeditor.resourceeditor.createNew</result>
			<interceptor-ref name="japsDefaultStack"><param name="requestAuth.requiredPermission">superuser</param></interceptor-ref>
		</action>

		<action name="saveNew" class="jpstaticresourceeditorResourceeditorAction" method="createNew">
			<result name="input" type="tiles">admin.jpstaticresourceeditor.resourceeditor.createNew</result>
			<result type="tiles">admin.jpstaticresourceeditor.resourceeditor.createNew</result>
			 <result type="redirect-action">
			 	<param name="actionName">list</param>
			 </result>
			<interceptor-ref name="japsDefaultStack"><param name="requestAuth.requiredPermission">superuser</param></interceptor-ref>
		</action>

	</package>

</struts>
